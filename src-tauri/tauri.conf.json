{
    "$schema": "https://schema.tauri.app/config/2",
    "productName": "flexo-assistant",
    "version": "0.1.0",
    "identifier": "com.flexo-assistant.app",
    "build": {
        "beforeDevCommand": "yarn dev",
        "devUrl": "http://localhost:1420",
        "beforeBuildCommand": "yarn build",
        "frontendDist": "../dist"
    },
    "app": {
        "windows": [
            {
                "title": "柔印印前排版小助手",
                "resizable": false,
                "width": 420,
                "height": 660
            }
        ],
        "security": {
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ]
    }
}
